  "scripts": {
    "start": "npm run dev",
    "test": "vite --mode testing",
    "dev": "vite --mode development",
    "build": "vite build --mode production",
    "preview": "vite preview --port 4173",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .eslintignore",
    "format": "prettier --w . --ignore-path .eslintignore",
    "pull": "git pull & npm run submodule:pull",
    "commit": "git add . & git commit -m [update]自动提交",
    "push": "git push",
    "submodule:init": "git submodule update --init --recursive",
    "submodule:pull": "cd branch-pages & git pull origin pages",
    "submodule:push": "cd branch-pages & git add . & git commit -m [update]branch-pages & git push"
  },
