    "push:pages": "git subtree push --prefix dist origin pages"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.13",
    "dayjs": "^1.11.6",
    "pinia": "^2.0.21",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.3",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/test-utils": "^2.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "phaser": "^3.55.2",
    "terser": "^5.15.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^3.0.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vitest": "^0.23.0"
